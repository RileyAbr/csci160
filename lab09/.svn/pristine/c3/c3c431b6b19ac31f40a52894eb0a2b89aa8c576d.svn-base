package Questions;

import ProvidedClasses.Animal;
import java.awt.Color;

/**
 *
 * @author riley
 */
public class Bird extends Animal{
    
    protected String name;
    protected int birthYear;
    protected Color featherColor;
    
    public Bird()
    {
        super();
        featherColor = Color.BLUE;
    }
    
    public Bird(String newName, int newBirthYear, Color newFeatherColor)
    {
        super(newName, newBirthYear);
        featherColor = newFeatherColor;
    }
    
    public Color getFeatherColor()
    {
        return featherColor;
    }
    
    @Override
    public String toString()
    {
        return super.toString() + ", " + featherColor; 
    }
}
